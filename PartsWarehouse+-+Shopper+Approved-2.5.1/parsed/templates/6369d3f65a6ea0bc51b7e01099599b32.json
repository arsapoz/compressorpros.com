{"components/custom/add-vehicle":"{{#each categories}}\n    {{#if this.name '==' ../theme_settings.hide_category}}\n        <span class=\"default-category\" data-default-category-name=\"{{this.name}}\">{{this.id}}</span>\n    {{/if}}\n{{/each}}\n<div class=\"vch-mobile-view\">\n    <div class=\"selection\">\n        <p>{{lang 'common.filter_text'}}</p>\n    </div>\n    <div class=\"buttons\">\n        <span class=\"vch-reset\">\n            <svg version=\"1.1\" xmlns=\"http://www.w3.org/2000/svg\" width=\"32\" height=\"32\" viewBox=\"0 0 32 32\">\n                <path d=\"M0 2.6l13.4 13.4-13.4 13.3 2.6 2.6 13.4-13.4 13.4 13.4 2.6-2.6-13.4-13.4 13.4-13.4-2.6-2.5-13.4 13.4-13.3-13.4-2.7 2.6z\"></path>\n            </svg>\n        </span>\n        <span class=\"vch-change\">{{lang 'common.filter_change_text'}}</span>\n    </div>\n</div>\n<div class=\"vch-main hide-filter\">\n    <div class=\"vch-filter\">\n        <label class=\"form-label-text\">\n            <h3 class=\"title\">\n                <svg version=\"1.1\" xmlns=\"http://www.w3.org/2000/svg\" width=\"32\" height=\"32\" viewBox=\"0 0 32 32\">\n                    <path d=\"M19.3 12.2h11.5v6.7h-11.5v13.1h-7.1v-13.1h-11.6v-6.7h11.5v-12.2h7.1v12.2z\"></path>\n                </svg>\n                {{lang 'common.filter_shop_for_text'}}</h3>\n            <span class=\"desc\"></span>\n        </label>\n        <div class=\"form-wrapper-vch {{#if theme_settings.show-fourthlevel '===' 4}}field-shown{{/if}}\">\n            <div class=\"form-field\">\n                <select id=\"vch-level-1\" class=\"form-select\">\n                    <option selected=\"\" disabled=\"\" data-default-selection=\"{{lang 'common.filter_first_level'}}\">{{lang 'common.filter_first_level'}}</option>\n                </select>\n            </div>\n            <div class=\"form-field\">\n                <select id=\"vch-level-2\" class=\"form-select\">\n                    <option selected=\"\" disabled=\"\" data-default-selection=\"{{lang 'common.filter_second_level'}}\">{{lang 'common.filter_second_level'}}</option>\n                </select>\n            </div>\n            <div class=\"form-field\">\n                <select id=\"vch-level-3\" class=\"form-select\">\n                    <option selected=\"\" disabled=\"\" data-default-selection=\"{{lang 'common.filter_third_level'}}\">{{lang 'common.filter_third_level'}}</option>\n                </select>\n            </div>\n            {{#if theme_settings.show-fourthlevel '===' 4}}\n            <div class=\"form-field\">\n                <select id=\"vch-level-4\" class=\"form-select\">\n                    <option selected=\"\" disabled=\"\" data-default-selection=\"{{lang 'common.filter_fourth_level'}}\">{{lang 'common.filter_fourth_level'}}</option>\n                </select>\n            </div>\n            {{/if}}\n        </div>\n        <div class=\"group-button\">\n            <a class=\"button button--primary srch-btn\" id=\"vch-select-search\" title=\"Search\"><span>{{lang 'common.filter_search_text'}}</span></a>\n            <a class=\"rst-link vch-reset\" title=\"Reset\"><span>{{lang 'common.filter_search_reset'}}</span></a>\n        </div>\n    </div>\n</div>\n"}
